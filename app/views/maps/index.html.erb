<div class="container">
    <div class="col-md-3">
    <header><h1>Midways</h1></header>
      <form class="form-horizontal" action="#">

        <label for="astart" class="mdl-textfield__label">Your location</label>      
        <div class="form-group">
            <div class="col-sm-12">
              <input type="text" class="form-control" id="start" placeholder="Your Address" value='Current Location'>
            </div>
        </div>

        <label for="bstart" class="mdl-textfield__label">Friend's location</label>
        <div class="form-group">
            <div class="col-sm-12">
              <input type="text" class="form-control" id="end" placeholder="Friend's Address" value='101 Cooper St, New York, NY'>
            </div>
        </div>

        <label for="meetTime" class="mdl-textfield__label">Meet by</label>
        <div class="form-group">
            <div class="col-sm-12">
              <input type="time" class="form-control" id="meetTime" placeholder="Time of meet up">
            </div>
        </div>

        <label class="mdl-textfield__label" for="cuisine">What would you like to eat?</label>
          <select class="form-control">
            <option value="barbecue">Barbecue</option>
            <option value="french">French</option>
            <option value="indian">Indian</option>
            <option value="japanese">Japanese</option>
            <option value="pizza">Pizza</option>
            <option value="steak">Steak</option>
            <option value="thai">Thai</option>
            <option value="american">American</option>
            <option value="chinese">Chinese</option>
            <option value="hamburger">Hamburger</option>
            <option value="italian">Italian</option>
            <option value="mexican">Mexican</option>
            <option value="seafood">Seafood</option>
          </select>

      <div class="form-group">
        <div class="col-sm-offset-8 col-sm-4">
          <button type="submit" class="btn btn-default plan-it">Plan It</button>
        </div>
      </div>

       </form> 
      </div>

    <div class="map">  
      <div id="map"></div>
    </div>  
   </div>